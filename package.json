{
	"name": "obsidian-card-navigator",
	"version": "1.2.6",
	"description": "노트를 시각화하고 탐색하는 독특한 방법을 제공하는 Obsidian 플러그인입니다.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "vitest",
		"test:watch": "vitest watch",
		"test:coverage": "vitest run --coverage"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"card",
		"navigator",
		"visualization"
	],
	"author": "wakeyi",
	"license": "MIT",
	"devDependencies": {
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^16.11.6",
		"@types/react": "^19.1.1",
		"@types/react-dom": "^19.1.2",
		"@typescript-eslint/eslint-plugin": "^5.29.0",
		"@typescript-eslint/parser": "^5.29.0",
		"builtin-modules": "^3.2.0",
		"esbuild": "0.17.3",
		"jsdom": "^26.0.0",
		"obsidian": "latest",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"tslib": "^2.8.1",
		"typescript": "4.7.4",
		"vitest": "^3.1.1"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.8",
		"@types/handlebars": "^4.0.40",
		"@types/uuid": "^10.0.0",
		"handlebars": "^4.7.8",
		"i18next": "^23.15.1",
		"inversify": "^7.5.0",
		"lucide": "^0.485.0",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.2",
		"tsyringe": "^4.9.1",
		"uuid": "^11.1.0"
	}
}
